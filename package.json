{
	"name": "etherlab-nodejs",
	"version": "0.0.2",
	"description": "Bind Etherlab's ethercat master to node.js",
	"main": "lib/ecat.lib.js",
	"bin": {
		"ecat-run": "bin/cli.js",
		"ecat-config": "bin/configure.js"
	},
	"scripts": {
		"preinstall": "rm -rf build/",
		"postinstall": "node-gyp rebuild",
		"build_debug": "export BUILD_TARGET=\"DEBUG\" && npm install"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/STECHOQ/etherlab-nodejs"
	},
	"os": [
		"linux"
	],
	"keywords": [
		"etherlab",
		"ethercat"
	],
	"author": "iqrok",
	"license": "LGPLv2.1",
	"dependencies": {
		"bindings": "^1.5.0",
		"node-addon-api": "^4.0.0",
		"node-gyp": "^8.1.0"
	}
}
